{
    "name": "integralwebmaster/youtube",
    "description": "Upload a video to YouTube with Laravel 6.",
    "license": "MIT",
    "authors": [
        {
            "name": "Hicham Abattay",
            "email": "integral.webmaster@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0",
        "google/apiclient": "^2.4"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^9.0",
        "phpunit/phpcov": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Dawson\\Youtube\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dawson\\Youtube\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Dawson\\Youtube\\YoutubeServiceProvider"
            ],
            "aliases": {
                "Youtube": "Dawson\\Youtube\\Facades\\Youtube"
            }
        }
    }
}
